@{
    ViewData["Title"] = "Error";
}

<h1>Error</h1>

<p>An error occurred while importing users:</p>

<p>Status Code: @ViewBag.StatusCode</p>
<p>Reason Phrase: @ViewBag.ReasonPhrase</p>
<p>Error Message: @ViewBag.ErrorMessage</p>

@if (!string.IsNullOrEmpty(ViewBag.ErrorContent))
{
    <p>Error Content:</p>
    <p>@ViewBag.ErrorContent</p>
}